{
    "name": "wilocampo/modular-php74",
    "description": "Modularize your Laravel apps - PHP 7.4 compatible fork",
    "keywords": [
        "laravel",
        "modules",
        "modular",
        "module"
    ],
    "authors": [
        {
            "name": "Chris Morrell",
            "homepage": "http://www.cmorrell.com"
        },
        {
            "name": "PHP 7.4 Compatibility Fork",
            "homepage": "https://github.com/wilocampo/modular-php74"
        }
    ],
    "support": {
        "source": "https://github.com/wilocampo/modular-php74",
        "issues": "https://github.com/wilocampo/modular-php74/issues"
    },
    "homepage": "https://github.com/wilocampo/modular-php74",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "composer/composer": "^2.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.52|^8.33|^9.11|^10.0|dev-master|dev-main",
        "friendsofphp/php-cs-fixer": "^3.14",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5|^10.5|^11.5",
        "ext-json": "*",
        "livewire/livewire": "^2.5|^3.0"
    },
    "autoload": {
        "psr-4": {
            "InterNACHI\\Modular\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InterNACHI\\Modular\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "InterNACHI\\Modular\\Support\\ModularServiceProvider",
                "InterNACHI\\Modular\\Support\\ModularizedCommandsServiceProvider",
                "InterNACHI\\Modular\\Support\\ModularEventServiceProvider"
            ],
            "aliases": {
                "Modules": "InterNACHI\\Modular\\Support\\Facades\\Modules"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
